<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <grid name="email-address-grid" title="Email addresses"
    model="com.axelor.base.db.EmailAddress">
    <field name="fullEmail"/>
    <field name="userName"/>
    <field name="domainName"/>
    <field name="priority"/>
    <field name="isOutdated"/>
  </grid>


  <form name="email-address-form" title="Email address" model="com.axelor.base.db.EmailAddress">
    <panel name="mainPanel">
      <field name="fullEmail"/>
      <field name="userName"/>
      <field name="domainName"/>
      <field name="priority"/>
      <field name="isOutdated"/>
    </panel>
  </form>

</object-views>
