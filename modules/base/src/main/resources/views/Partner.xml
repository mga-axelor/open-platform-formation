<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <grid name="partner-grid" title="Partners" model="com.axelor.base.db.Partner" orderBy="id">
    <field name="partnerSeq"/>
    <field name="typeSelect"/>
    <field name="civility"/>
    <field name="firstName"/>
    <field name="name"/>
    <field name="mainPhoneNumber"/>
    <field name="secondaryPhoneNumber"/>
  </grid>

  <form name="partner-form" title="Partner" model="com.axelor.base.db.Partner">
    <panel name="mainPanel">
      <field name="partnerSeq"/>
      <field name="typeSelect"/>
      <field name="civility"/>
      <field name="firstName"/>
      <field name="name"/>
      <field name="addressList"/>
      <field name="emailAddressList"/>
      <field name="mainPhoneNumber"/>
      <field name="secondaryPhoneNumber"/>
    </panel>
  </form>

</object-views>
