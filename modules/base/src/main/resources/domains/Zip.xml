<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="base" package="com.axelor.base.db"/>

  <entity name="Zip">

    <!-- Will contain concatenation of Zip & CityName -->
    <string name="name" title="Name" readonly="true" namecolumn="true"/>

    <string name="code" title="Code" required="true"/>
    <many-to-one name="city" ref="com.axelor.base.db.City" required="true"/>

    <unique-constraint columns="code,city"/>

    <entity-listener class="com.axelor.apps.base.listener.ZipListener"/>

  </entity>

</domain-models>
