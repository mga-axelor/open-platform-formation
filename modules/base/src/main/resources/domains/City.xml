<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="base" package="com.axelor.base.db"/>

  <entity name="City">

    <string name="name" title="Name" namecolumn="true" required="true"/>
    <many-to-one name="country" title="Country" ref="com.axelor.base.db.Country"
      required="true"/>

    <one-to-many name="zipList" title="Zips" ref="com.axelor.base.db.Zip"
      mappedBy="city"/>

  </entity>

</domain-models>
